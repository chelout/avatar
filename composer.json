{
    "name": "chelout/avatar",
    "description": "Turn name, email, and any other string into beautiful, initial-based avatar with shadow.",
    "keywords": [
        "laravel",
        "avatar"
    ],
    "homepage": "https://github.com/chelout/avatar",
    "license": "MIT",
    "authors": [
        {
            "name": "Viacheslav Ostrovskiy",
            "email": "chelout@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": "5.5.x|5.6.x",
        "illuminate/cache": "5.5.x|5.6.x",
        "intervention/image": "^2.1",
        "danielstjules/stringy": "~3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "^0.9.1"
    },
    "autoload": {
        "psr-4": {
            "Chelout\\Avatar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chelout\\Avatar\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chelout\\Avatar\\ServiceProvider"
            ],
            "aliases": {
                "Avatar": "Chelout\\Avatar\\Facade"
            }
        }
    }
}
